---
import { Picture } from "@astrojs/image/components";
import FooterImage from "~/assets/footer-grass.webp";

const widths = [450, 800, 1200];
const sizes = "100vw";
---

<Picture
  class="h-full w-full object-cover"
  src={FooterImage}
  widths={widths}
  sizes={sizes}
  alt="The grassy field"
/>
